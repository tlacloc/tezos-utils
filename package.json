{
  "name": "tezos-utils",
  "version": "1.0.0",
  "description": "scripts for develop smart contracts in tezos chain using smart-py CLI and taquito",
  "main": "index.js",
  "scripts": {
    "test": "node scripts/commands.js test",
    "deploy:local": "PROTOCOL_NAME=local ./scripts/commands.js deploy calculator",
