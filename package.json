    "deploy:mainnet": "PROTOCOL_NAME=telosMainnet ./scripts/commands.js deploy calculator",
    "compile:calculator" :"node scripts/program.js compile calculator",
    "compile:all" :"node scripts/program.js compile all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlacloc/tezos-utils.git"
  },
