  },
  "homepage": "https://github.com/tlacloc/tezos-utils#readme",
  "dependencies": {
    "@taquito/signer": "^11.2.0",
    "@taquito/taquito": "^11.2.0",
    "commander": "^3.0.2",
    "dotenv": "^16.0.0",
    "fs-extra": "^10.0.0",
    "prompt-sync": "^4.2.0",
    "uuid": "^8.3.2"
  }
